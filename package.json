{
  "name": "NubbleNH",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "prettier ./src --write && eslint ./src",
    "lint-fix": "prettier --write ./src && eslint --fix ./src",
    "start": "react-native start",
    "test": "jest",
    "android-clean": "cd android && ./gradlew clean && rm -rf ./build && cd ..",
    "node-clean": "rm -rf yarn.lock && rm -rf bun.lockb && rm -rf node_modules",
    "pod-install": "cd ios && pod install && cd ../",
    "generate-apk": "cd android && ./gradlew assembleRelease && cd app/build/outputs/apk/release && adb install app-release.apk && cd ../../../../",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.16",
    "@shopify/restyle": "^2.4.2",
    "react": "18.2.0",
    "react-hook-form": "^7.47.0",
    "react-native": "0.72.7",
    "react-native-safe-area-context": "^4.7.2",
    "react-native-screens": "^3.27.0",
    "react-native-svg": "^13.13.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@rnx-kit/babel-preset-metro-react-native": "^1.1.6",
    "@tsconfig/react-native": "^3.0.0",
    "@types/metro-config": "^0.76.3",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "eslint-plugin-import": "^2.29.0",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
